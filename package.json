{
  "name": "travelist",
  "private": true,
  "version": "1.0.0",
  "description": "Travelist - Travel recommendation management iOS app",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "NODE_ENV=production vite build",
    "build:sync": "npm run build && npx cap sync ios",
    "lint": "eslint .",
    "preview": "vite preview",
    "cap:sync": "npx cap sync ios",
    "cap:copy:ios": "npx cap copy ios && node scripts/ensure-shared-inbox-plugin.js",
    "cap:open": "npx cap open ios",
    "cap:run": "npx cap run ios",
    "ios:dev": "npm run build:dev && NODE_ENV=development npx cap sync ios && npx cap open ios",
    "ios:prod": "npm run build:prod && NODE_ENV=production npx cap sync ios && npx cap open ios",
    "share:test": "xcrun simctl openurl booted 'travelist://share?text=Amazing+restaurant+in+Paris+France'",
    "share:test:url": "xcrun simctl openurl booted 'travelist://share?text=https://maps.google.com/maps?q=Le+Cinq+Paris+France'"
  },
